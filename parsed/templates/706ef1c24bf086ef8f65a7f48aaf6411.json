{"components/common/quick-search":"<div class=\"container\">\r\n    <form class=\"form\" action=\"{{urls.search}}\" >\r\n        <fieldset class=\"form-fieldset\">\r\n            <div class=\"form-field category_search_products\">\r\n                <select class=\"form-select\" name=\"category\" id=\"category_id\">\r\n\t\t\t\t\t<option value=\"680\">All</option>\r\n\t\t\t\t\t{{#each categories}}\r\n\t\t\t\t\t    {{#if children}}\r\n\t\t\t\t\t        <option value=\"{{id}}\" class=\"search-main-cat\">{{name}}</option>\r\n\t\t\t\t\t        {{#each children}}\r\n\t\t\t\t\t        <option value=\"{{id}}\" class=\"search-child-cat\">¦&nbsp;&nbsp;&nbsp;&nbsp;{{name}}</option>\r\n\t\t\t\t\t        {{/each}}\r\n\t\t\t\t\t    {{else}}\r\n\t\t\t\t\t        <option value=\"{{id}}\" class=\"search-main-cat\">{{name}}</option>\r\n\t\t\t\t\t    {{/if}}\r\n\t\t\t\t\t{{/each}}\r\n\t\t\t\t</select>\r\n            </div>\r\n            <div class=\"form-field\">\r\n                <label class=\"is-srOnly\" for=\"{{name}}\">{{lang \"search.quick_search.input_label\"}}</label>\r\n                <input class=\"form-input\"\r\n                       data-search-quick\r\n                       name=\"search_query\"\r\n                       id=\"search_query\"\r\n                       data-error-message=\"{{lang 'search.error.empty_field'}}\"\r\n                       placeholder=\"Search for products or artists…\"\r\n                       autocomplete=\"off\"\r\n                       required=\"required\"\r\n                >\r\n                 <button class=\"button search_submit\" type=\"submit\">\r\n                     {{!--<svg xmlns=\"http://www.w3.org/2000/svg\" xmlns:xlink=\"http://www.w3.org/1999/xlink\" width=\"27.299\" height=\"28\" viewBox=\"0 0 27.299 28\">\r\n                  <defs>\r\n                    <clipPath id=\"clip-path\">\r\n                      <rect id=\"Rectangle_41\" data-name=\"Rectangle 41\" width=\"27.299\" height=\"28\" fill=\"#112928\"/>\r\n                    </clipPath>\r\n                  </defs>\r\n                  <g id=\"search_icon\" clip-path=\"url(#clip-path)\">\r\n                    <path id=\"Path_59\" data-name=\"Path 59\" d=\"M27.014,26.336l-5.068-5.068A12.635,12.635,0,1,0,20.517,22.6l5.118,5.118a.974.974,0,0,0,1.378-1.378M1.949,12.669a10.72,10.72,0,1,1,10.72,10.72,10.733,10.733,0,0,1-10.72-10.72\" transform=\"translate(0 0)\" fill=\"#112928\"/>\r\n                  </g>\r\n                </svg>--}}\r\n                <img src=\"https://store-haxq1vvmi9.mybigcommerce.com/content/search_icon.png\" alt=\"Search\" />\r\n                </button>\r\n            </div>\r\n        </fieldset>\r\n    </form>\r\n    <section class=\"quickSearchResults\" data-bind=\"html: results\"></section>\r\n    <p role=\"status\"\r\n       aria-live=\"polite\"\r\n       class=\"aria-description--hidden\"\r\n       data-search-aria-message-predefined-text=\"{{{lang 'search.results.quick_count_live'}}}\"\r\n    ></p>\r\n</div>\r\n"}
